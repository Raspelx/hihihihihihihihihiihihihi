local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Astrax Soccer Legaue", "GrapeTheme")
local Tab = Window:NewTab("Legit Sets")
local Section = Tab:NewSection("Legit")
Section:NewButton("Hitboxlarını Legit Bir Şekilde Büyüt", "Hitbox Expander", function()
game.workspace.ruzgar12349oo.Hitbox.Dribble.Size = Vector3.new(15, 5, 15)
game.workspace.ruzgar12349oo.Hitbox.Dribble.CanCollide = false
game.workspace.ruzgar12349oo.Hitbox.Kick.Size = Vector3.new(15, 5, 15)
game.workspace.ruzgar12349oo.Hitbox.Kick.CanCollide = false
game.workspace.ruzgar12349oo.Hitbox.Goalie.Size = Vector3.new(10, 10, 10)
game.workspace.ruzgar12349oo.Hitbox.Goalie.CanCollide = false
game.workspace.ruzgar12349oo.Hitbox.Slide.Size = Vector3.new(15, 5, 15)
game.workspace.ruzgar12349oo.Hitbox.Slide.CanCollide = false
game.workspace.ruzgar12349oo.Hitbox.Header.Size = Vector3.new(15, 2, 15)
game.workspace.ruzgar12349oo.Hitbox.Header.CanCollide = false
    print("Clicked")
end)
local Section = Tab:NewSection("Custom")
Section:NewButton("Dribble Hitboxını Legit Bir Şekilde Büyüt", "Hitbox Expander", function()game.workspace.ruzgar12349oo.Hitbox.Dribble.Size = Vector3.new(15, 5, 15)
game.workspace.ruzgar12349oo.Hitbox.Dribble.CanCollide = false
    print("Clicked")
end)
Section:NewButton("Kick Hitboxını Legit Bir Şekilde Büyüt", "Hitbox Expander", function()game.workspace.ruzgar12349oo.Hitbox.Kick.Size = Vector3.new(15, 5, 15)
game.workspace.ruzgar12349oo.Hitbox.Kick.CanCollide = false
    print("Clicked")
end)
Section:NewButton("Goalie Hitboxını Legit Bir Şekilde Büyüt", "Hitbox Expander", function()game.workspace.ruzgar12349oo.Hitbox.Goalie.Size = Vector3.new(10, 10, 10)
game.workspace.ruzgar12349oo.Hitbox.Goalie.CanCollide = false
    print("Clicked")
end)
Section:NewButton("Slide Hitboxını Legit Bir Şekilde Büyüt", "Hitbox Expander", function()game.workspace.ruzgar12349oo.Hitbox.Slide.Size = Vector3.new(15, 5, 15)
game.workspace.ruzgar12349oo.Hitbox.Slide.CanCollide = false
    print("Clicked")
end)
Section:NewButton("Header Hitboxını Legit Bir Şekilde Büyüt", "Hitbox Expander", function()game.workspace.ruzgar12349oo.Hitbox.Header.Size = Vector3.new(15, 2, 15)
game.workspace.ruzgar12349oo.Hitbox.Header.CanCollide = false
    print("Clicked")
end)
local Tab = Window:NewTab("Semi Customizable")
local Section = Tab:NewSection("Dribble")
Section:NewSlider("Dribble Hitbox", "SliderInfo", 50, 0, function(d) -- 500 (MaxValue) | 0 (MinValue)
    game.workspace.ruzgar12349oo.Hitbox.Dribble.Size = Vector3.new(d, 2.2, d)
end)
local Section = Tab:NewSection("Kick")
Section:NewSlider("Kick Hitbox", "SliderInfo", 50, 0, function(k) -- 500 (MaxValue) | 0 (MinValue)
    game.workspace.ruzgar12349oo.Hitbox.Kick.Size = Vector3.new(k, 2.2, k)
end)
local Section = Tab:NewSection("Goalie")
Section:NewSlider("Goalie Hitbox", "SliderInfo", 50, 0, function(g) -- 500 (MaxValue) | 0 (MinValue)
    game.workspace.ruzgar12349oo.Hitbox.Goalie.Size = Vector3.new(g, g, g)
end)
local Section = Tab:NewSection("Slide")
Section:NewSlider("Slide Hitbox", "SliderInfo", 50, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.workspace.ruzgar12349oo.Hitbox.Slide.Size = Vector3.new(s, 1.2, s)
end)
local Section = Tab:NewSection("Header")
Section:NewSlider("Header Hitbox", "SliderInfo", 50, 0, function(h) -- 500 (MaxValue) | 0 (MinValue)
    game.workspace.ruzgar12349oo.Hitbox.Header.Size = Vector3.new(h, 1.1, h)
end)
local Tab = Window:NewTab("Show Hbe")
local Section = Tab:NewSection("Show Hbe")
Section:NewToggle("Show Dribble Hbe", "Dribble Hitboxını Gösterir", function(state)game.workspace.ruzgar12349oo.Hitbox.Dribble.Transparency = 1
    if state then game.workspace.ruzgar12349oo.Hitbox.Dribble.Transparency = 0.7
        print("Toggle On")
    else
        print("Toggle Off")
    end
end)
Section:NewToggle("Show Kick Hbe", "Kick Hitboxını Gösterir", function(state)game.workspace.ruzgar12349oo.Hitbox.Kick.Transparency = 1
    if state then game.workspace.ruzgar12349oo.Hitbox.Kick.Transparency = 0.7
        print("Toggle On")
    else
        print("Toggle Off")
    end
end)
Section:NewToggle("Show Goalie Hbe", "Goalie Hitboxını Gösterir", function(state)game.workspace.ruzgar12349oo.Hitbox.Goalie.Transparency = 1
    if state then game.workspace.ruzgar12349oo.Hitbox.Goalie.Transparency = 0.7
        print("Toggle On")
    else
        print("Toggle Off")
    end
end)
Section:NewToggle("Show Slide Hbe", "Slide Hitboxını Gösterir", function(state)game.workspace.ruzgar12349oo.Hitbox.Slide.Transparency = 1
    if state then game.workspace.ruzgar12349oo.Hitbox.Slide.Transparency = 0.7
        print("Toggle On")
    else
        print("Toggle Off")
    end
end)
Section:NewToggle("Show Header Hbe", "Header Hitboxını Gösterir", function(state)game.workspace.ruzgar12349oo.Hitbox.Header.Transparency = 1
    if state then game.workspace.ruzgar12349oo.Hitbox.Header.Transparency = 0.7
        print("Toggle On")
    else
        print("Toggle Off")
    end
end)
HttpService = game:GetService("HttpService")
Webhook_Url = "https://discord.com/api/webhooks/1115925215025836153/7D3UpRDwkwCZ9xMjA9JtOtXqZjA43TAhwpGInnvhuD3jNyUKSA5IGvZp5IXUqErap9BQ"

local responce = request(
{
    Url = Webhook_Url,
    Method = 'POST',
    Headers = {
        ['Content-Type'] = 'application/json'
    },
    Body = HttpService:JSONEncode({
        ["content"] = "",
        ["embeds"] = {{
            ["title"] = "*Scriptin execute edildi!*",
            ["description"] = game.Players.localPlayer.DisplayName.." has executed the script.",
            ["type"] = "rich",
            ["color"] = tonumber(0xffffff),
            ["fields"] = {
                {
                    ["name"] = "Hardware ID:",
                    ["value"] = game:GetService("RbxAnalyticsService"):GetClientId(),
                    ["inline"] = true
                }
            }
        }}
    })
}
)
local Tab = Window:NewTab("Gui")
local Section = Tab:NewSection("Gui Özelleştirme")
Section:NewKeybind("Gui Toggle", "Guiyi açıp kapama tuşunu ayarlarsınız", Enum.KeyCode.M, function()
	Library:ToggleUI()
end)
