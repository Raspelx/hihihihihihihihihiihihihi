local Mercury = loadstring(game:HttpGet("https://raw.githubusercontent.com/deeeity/mercury-lib/master/src.lua"))()

local GUI = Mercury:Create{
    Name = "Astrax",
    Size = UDim2.fromOffset(600, 400),
    Theme = Mercury.Themes.Legacy,
    Link = "https://github.com/deeeity/mercury-lib"
}
local Tab = GUI:Tab{
	Name = "Reach",
	Icon = "rbxassetid://8569322835"
}
Tab:Button{
	Name = "Hitboxlarını Legit Bir Şekilde Büyüt",
	Description = nil,
	Callback = function()game.workspace.ruzgar12349oo.Hitbox.Dribble.Size = Vector3.new(15, 5, 15)
game.workspace.ruzgar12349oo.Hitbox.Dribble.CanCollide = false
game.workspace.ruzgar12349oo.Hitbox.Kick.Size = Vector3.new(15, 5, 15)
game.workspace.ruzgar12349oo.Hitbox.Kick.CanCollide = false
game.workspace.ruzgar12349oo.Hitbox.Goalie.Size = Vector3.new(10, 10, 10)
game.workspace.ruzgar12349oo.Hitbox.Goalie.CanCollide = false
game.workspace.ruzgar12349oo.Hitbox.Slide.Size = Vector3.new(15, 5, 15)
game.workspace.ruzgar12349oo.Hitbox.Slide.CanCollide = false
game.workspace.ruzgar12349oo.Hitbox.Header.Size = Vector3.new(15, 2, 15)
game.workspace.ruzgar12349oo.Hitbox.Header.CanCollide = false end
}
local Tab = GUI:Tab{
